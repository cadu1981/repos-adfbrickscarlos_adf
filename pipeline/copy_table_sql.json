				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"sink": {
						"type": "AzureSqlSink",
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false
				},
				"outputs": [
					{
						"referenceName": "ds_sqldatabase_generico",
						"type": "DatasetReference",
						"parameters": {
							"SchemaName": "treinamento_adfbricks",
							"TableName": "MoviesTable"
						}
					}
				]
			}
		],
		"annotations": []
	}
}